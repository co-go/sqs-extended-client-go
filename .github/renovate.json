{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    ":ignoreModulesAndTests",
    "group:recommended",
    "replacements:all",
    "workarounds:all",
    ":automergeBranch"
  ],
  "automerge": true,
  "postUpdateOptions": [
    "gomodTidy"
  ],
  "packageRules": [
    {
      "extends": [
        "monorepo:aws-sdk-go-v2"
      ],
      "groupName": "aws-sdk-go-v2 monorepo",
      "matchUpdateTypes": [
        "digest",
        "patch",
        "minor",
        "major"
      ],
      "matchPackageNames": [
        "!github.com/aws/aws-sdk-go-v2/service/sqs"
      ]
    },
    {
      "extends": [
        ":automergeDisabled"
      ],
      "matchPackageNames": [
        "github.com/aws/aws-sdk-go-v2/service/sqs"
      ]
    }
  ]
}
